{
  "name": "onchain-json-traits",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "rm-macosx-system-files": "find . -name '.DS_Store' -type f -delete",
    "generateAssetJson": "yarn ts-node ./scripts/generateAssetJson.ts",
    "minifySvg": "yarn ts-node ./scripts/minify-svg.ts  ",
    "test": "npx hardhat test",
    "test:all": "yarn run rm-macosx-system-files && yarn run generateAssetJson && yarn run minifySvg && yarn run test",
    "node": "npx hardhat node",
    "deploy": "npx hardhat run ./scripts/deploy.ts --network localhost",
    "start-web": "ts-node ./scripts/serve.ts"
  },
  "devDependencies": {
    "@fleekhq/fleek-storage-js": "^1.0.22",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/chai": "^4.3.3",
    "@types/jest": "^29.0.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "@types/pako": "^2.0.0",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.1",
    "express": "^4.18.1",
    "files-from-path": "^0.2.6",
    "hardhat": "^2.11.1",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.9",
    "nft.storage": "^7.0.0",
    "pako": "^2.0.4",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "^0.6.0-beta.25",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.3"
  }
}
